{
  "name": "navy",
  "version": "1.0.0-rc1",
  "description": "Docker based dev environments made easy",
  "main": "lib/index.js",
  "bin": {
    "navy": "./bin/navy.js"
  },
  "options": {
    "cucumber": "--strict --fail-fast -r ./preload.js -r ./environment.js -r ./hooks.js -r ./chai.js -r ./features -r ./steps ./features"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "watch": "rm -rf lib && babel src -wd lib",
    "integration": "cd test/integration && cucumberjs $npm_package_options_cucumber",
    "lint": "eslint .",
    "flow": "flow",
    "test": "./scripts/test.sh",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momentumft/navy.git"
  },
  "keywords": [
    "docker",
    "environment"
  ],
  "author": "Momentum Financial Technology Ltd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/momentumft/navy/issues"
  },
  "homepage": "https://github.com/momentumft/navy#readme",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-properties": "^1.2.1",
    "cucumber": "^0.10.2",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-flow-vars": "^0.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "flow-bin": "^0.22.1",
    "pty.js": "^0.3.0",
    "strip-ansi": "^3.0.1"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "lodash": "^4.10.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "zygon": "^0.1.0"
  }
}
