{
  "name": "navy-www",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "less": "mkdir -p build/css; lessc styles/main.less build/styles.css",
    "html": "../node_modules/.bin/babel-node .",
    "copy-public": "cp -R public/* build/",
    "build": "npm run less; npm run copy-public; npm run html;",
    "watch": "nodemon -e js,html,less,md -w ../docs -w ./styles -w ./public -w ./layouts --exec npm run build"
  },
  "devDependencies": {
    "bluebird": "^3.4.1",
    "documentation": "^4.0.0-beta9",
    "front-matter": "^2.1.0",
    "glob": "^7.0.5",
    "less": "^2.7.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.10.0",
    "nunjucks": "^2.4.2",
    "showdown": "^1.4.2"
  }
}
